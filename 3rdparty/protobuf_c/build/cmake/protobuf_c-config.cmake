include(${CMAKE_CURRENT_LIST_DIR}/protobuf_c-targets.cmake)
set(PROTOBUF_C_TARGETS protobuf_c)
set(PROTOBUF_C_INCLUDE_DIRS)
set(PROTOBUF_C_LIBRARIES)
foreach(tgt ${PROTOBUF_C_TARGETS})
  if(TARGET ${tgt})
    get_property(TGT_INCLUDE TARGET ${tgt} PROPERTY INTERFACE_INCLUDE_DIRECTORIES) 
    list(APPEND PROTOBUF_C_INCLUDE_DIRS ${TGT_INCLUDE})
    get_property(TGT_LIBRARY TARGET ${tgt} PROPERTY LOCATION)
    list(APPEND PROTOBUF_C_LIBRARIES ${TGT_LIBRARY})
  else()
    MESSAGE(FATAL "${tgt is not a TARGET}")
  endif()
endforeach(tgt)
