  <param name="data_port" type= "int" value="$(arg data_port2)" />
  <param name="gps_port" type= "int" value="$(arg gps_port2)" />
  </node>


  <node pkg="holomatic" type="velodyne_driver" name="velodyne_driver4" output="screen" required="true">
  <param name="yaml_file" type= "string" value="$(arg yaml_file4)" />
  <param name="coord" type= "string" value="$(arg coord4)" />
  <param name="data_port" type= "int" value="$(arg data_port4)" />
  <param name="gps_port" type= "int" value="$(arg gps_port4)" />
  </node>
-->        
</launch>
